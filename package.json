{
  "name": "csu-gf-api",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.1",
    "@types/uuid": "^3.4.5",
    "gql2ts": "^1.10.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^4.0.6",
    "graphql-yoga": "^1.18.3",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.20",
    "uuid": "^3.3.3"
  },
  "scripts": {
